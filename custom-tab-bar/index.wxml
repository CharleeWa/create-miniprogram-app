<van-tabbar active="{{ active }}" data-key="{{active}}" bind:change="onChange" active-color="#0CB65B" inactive-color="#A3A8AB">
  <van-tabbar-item wx:for="{{ list }}" wx:key="index">
    <image slot="icon" src="{{ item.normal }}" mode="aspectFit" style="width: 48rpx; height: 48rpx;" />
    <image slot="icon-active" src="{{ item.active }}" mode="aspectFit" style="width: 48rpx; height: 48rpx;" />
    {{item.text}}
  </van-tabbar-item>
</van-tabbar>